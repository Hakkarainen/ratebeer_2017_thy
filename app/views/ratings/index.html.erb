
<h1>List of ratings</h1>
<p> Number of ratings <%= @ratings.count %> </p>

<ul>
    <% @ratings.each do |rating| %>
      <% u = @users.find_by id: rating.user_id %>
        <li><%= rating %> <%= link_to u.username, user_path(rating.user_id) %></li>
    <% end %>
</ul>

<p><%= link_to 'New Rating', new_rating_path %></p>

